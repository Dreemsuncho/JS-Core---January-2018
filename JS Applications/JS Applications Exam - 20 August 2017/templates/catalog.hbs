
{{> header }} 
{{> menu }}

<section id="viewCatalog">
    <div class="posts">
        {{#if posts}}
            {{#each posts }}
                {{> post }}
            {{/each}}
        {{else}}
        <article class="post">
            <div class="clear">No posts in database</div>
        </article>
        {{/if}}
    </div>
</section>

{{> footer }}